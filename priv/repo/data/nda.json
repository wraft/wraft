            "attrs": {
                "align": null,
                "id": null,
                "indent": 0,
                "lineSpacing": null
            },
            "content": [
                {
                    "type": "text",
                    "marks": [
                        {
                            "type": "bold"
                        }
                    ],
                    "text": "Employee Signature"
                },
                {
                    "type": "text",
                    "text": " _____________________________________ Date: ________________"
                }
            ]
        },
